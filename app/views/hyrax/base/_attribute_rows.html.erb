<table class="metadata-table">
  <%= presenter.attribute_to_html(:title) %>
  <%= presenter.attribute_to_html(:alternative) %>
  <%= presenter.attribute_to_html(:series_title) %>
  <%= presenter.attribute_to_html(:creator, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:contributor, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:publisher, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:date) %>
  <%= presenter.attribute_to_html(:language, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:description) %>
  <%= presenter.attribute_to_html(:subject, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:place, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:time_period, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:genre, render_as: :faceted) %>
  <%= presenter.attribute_to_html(:format) %>
  <%= presenter.attribute_to_html(:resource_type, render_as: :faceted, label: t('simple_form.labels.defaults.resource_type')) %>
  <%= presenter.attribute_to_html(:extent) %>
  <%= presenter.attribute_to_html(:identifier) %>
  <%= presenter.attribute_to_html(:provenance, render_as: :faceted) %>
  <%= presenter.location_info %>
  <%= presenter.attribute_to_html(:donor) %>
  <%= presenter.attribute_to_html(:rights_statement, render_as: :rights_statement, label: t('simple_form.labels.defaults.rights_statement')) %>
  <%= presenter.attribute_to_html(:rights_holder, label: t('simple_form.labels.defaults.rights_holder')) %>
  <%= presenter.attribute_to_html(:note) %>
  <%= presenter.attribute_to_html(:related_item, label: t('simple_form.labels.defaults.related_item')) %>
  <%= presenter.attribute_to_html(:digital_object_ark , label: t('simple_form.labels.defaults.digital_object_ark')) %>
</table>
